@model Ogretmen    
<h2>Ogretmen Ekle</h2>
<hr>
<form asp-action="Create" asp-controller="Ogretmen" method="post">
    <div class="mb-3">
        <label asp-for="Ad" class="form-label"></label>
        <input asp-for="Ad" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Soyad" class="form-label"></label>
        <input asp-for="Soyad" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Eposta" class="form-label"></label>
        <input asp-for="Eposta" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Telefon" class="form-label"></label>
        <input asp-for="Telefon" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="BaslamaTarihi" class="form-label"></label>
        <input asp-for="BaslamaTarihi" class="form-control">
    </div>

    <button type="submit" class="btn btn-secondary">Kaydet</button>
</form>

